<div id="addservice" class="tabcontainer">
<div class="stepstitle">
   <span class="number boxsize">3</span>
   <span class="name boxsize dispinl">Add Service</span>

 </div>
<div class="setup-container boxsize">
   <div class="tables ">

         <div class="tablecell">
            Service Name
         </div>
         <div class="tablecell">
            Duration
         </div>
    </div>
    <div class="list-pages">

    </div>

    <div class="tables border-light">

       <div class="tablecell">
         <span class="inputbox dispinl fl">
              {{input type="text" placeholder="Service name" class="txt-input" name="serviceName" id="serviceName" }}

          </span>
       </div>
       <div class="tablecell">
         <span class="inputbox dispinl fl time">
               {{input placeholder="00" class="txt-input" name="time" id="time" }}
          </span>

          <span class="dispinl fl mins grey-color">Mins</span>
       </div>


    </div>
    <div class="tab-row">
        <a href="#" class="addService dispinl boxsize" {{action "addService"}}> Add Service</a>
        <a href="#" class="cancelService dispinl boxsize grey-color">Cancel</a>
    </div>
    <div class="btn-nxt boxsize fr">
        <button class="btnstyle dispinl"></button>
    </div>
</div>
</div>
